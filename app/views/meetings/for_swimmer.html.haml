.wrapper-body
  .spacer-left

  .main-content#top-of-page
    %section#all-meetings-title
      %h4.text-center
        %small
          = render(Title::SwimmerShowLinkComponent.new(swimmer: @swimmer))
          %i.text-muted
            &nbsp;
            %i.fa.fa-angle-right{ 'aria-hidden': 'true' }
            &nbsp;
            %span.text-dark
              %i.fa.fa-trophy{ 'aria-hidden': 'true' }
              = t('swimmers.radiography.all_meetings')

    = render(partial: 'meeting_grid', locals: { grid: @grid, grid_filter_params: @grid_filter_params })

  .spacer-right
